<!DOCTYPE html>
<html>
  <head>
    <link rel="stylesheet" type="text/css" href="/css/adminControls.css" />
  </head>
  <div style="col-sm-6">
  {{message}}
  </div>

  <div style="col-sm-6">
    <button> <a style="text-decoration:none;" href='makeReservation'>Make a Reservation</a></button>
  </div>

  {{#each user}}
  <div>
    {{this.user.name}}
  </div>
  <div>
  {{this.user.id}}
  </div>
  {{/each}}



  <div id="cont">
    <div id="tp">
      <table class="linkText">    
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>ID</th>    
        </tr>
        {{#each user}}
        <tr>
          
          <td>{{this.user.name}}</td>
          <td>{{this.user.primary_email.email}}</td>
          <td>{{this.user.id}} </td>
          
        </tr>
        {{/each}}
      </table>


    </div>
  </div>
</html>